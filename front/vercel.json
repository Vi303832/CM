{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://zynote.onrender.com/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/summary": {
            "maxDuration": 30
        }
    }
}